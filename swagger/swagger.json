{
    "swagger": "2.0",
    "info": {
        "description": "\"Starter code for Go application with Gin, System Tray, Gorm, Air, Swagger\"",
        "title": "gin-systray-starter",
        "contact": {
            "name": "Github Issues",
            "url": "https://github.com/jinyaoMa/gin-systray-starter/issues"
        },
        "license": {
